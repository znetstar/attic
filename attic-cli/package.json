{
    "name": "@znetstar/attic-cli",
    "version": "3.0.0",
    "contributors": [
        {
            "name": "Zachary R.T. Boyd",
            "email": "zachary@zacharyboyd.nyc",
            "url": "https://zacharyboyd.nyc"
        },
        {
            "name": "Etomon Team",
            "email": "open-source@etomon.com",
            "url": "https://etomon.com"
        }
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "bin": {
        "attic-cli": "./bin/run"
    },
    "homepage": "https://github.com/znetstar/attic",
    "bugs": "https://github.com/znetstar/attic/issues",
    "repository": {
        "url": "https://github.com/znetstar/attic.git",
        "type": "git"
    },
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.0",
        "@znetstar/attic-cli-common": "^3.0.0",
        "@znetstar/attic-cli-url-shortener": "^3.0.0",
        "@znetstar/attic-common": "^3.0.0",
        "multi-rpc": "^1.6.7",
        "tslib": "^1.13.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.2",
        "@types/node": "^14.6.2",
        "globby": "^10.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "keywords": [
        "oclif"
    ],
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "attic-cli",
        "plugins": [
            "@oclif/plugin-help",
            "@znetstar/attic-cli-url-shortener"
        ],
        "hooks": {
            "init": "./lib/hooks/init/config"
        }
    },
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "echo NO TESTS",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
